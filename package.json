{
  "name": "shopozor-manager",
  "version": "1.0.0",
  "description": "Admin interface to the Shopozor",
  "productName": "Shopozor Admin",
  "cordovaId": "org.cordova.quasar.app",
  "author": "Softozor",
  "private": true,
  "scripts": {
    "build": "quasar build",
    "cypress:open:dev": "cross-env CYPRESS_baseUrl=http://localhost:5000/# NODE_ENV=development cypress open",
    "cypress:open": "cross-env CYPRESS_baseUrl=http://localhost:4000/# cypress open",
    "cypress:integration": "wait-on http://localhost:4000 && cross-env CYPRESS_baseUrl=http://localhost:4000/# cypress run --env configFile=integration",
    "cypress:integration:dev": "wait-on http://localhost:5000 && cross-env CYPRESS_baseUrl=http://localhost:5000/# cypress run --env configFile=integration",
    "cypress:e2e": "wait-on http://localhost:4000 && cross-env CYPRESS_baseUrl=http://localhost:4000/# cypress run --env configFile=e2e",
    "start:integration": "concurrently \"yarn cypress:integration\" --kill-others \"yarn start\" --success first",
    "start:integration:dev": "concurrently \"yarn cypress:integration:dev\" --kill-others \"yarn start:dev\" --success first",
    "start:e2e": "concurrently \"yarn cypress:e2e\" --kill-others \"yarn start\" --success first",
    "lint": "eslint --ext .js,.vue src",
    "start": "quasar serve dist/spa -H '0.0.0.0' -p 4000",
    "start:dev": "quasar dev",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit:ci": "jest --updateSnapshot --ci --reporters=jest-junit",
    "test:unit": "jest  --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "jest-junit": {
    "outputDirectory": "./junit-reports",
    "outputName": "junit.xml"
  },
  "dependencies": {
    "@quasar/extras": "^1.3.2",
    "animate.css": "^3.7.2",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "google-translate-api": "^2.3.0",
    "graphql": "^14.5.7",
    "graphql-tag": "^2.10.1",
    "node-fetch": "^2.6.0",
    "quasar": "^1.1.4",
    "sinon": "^7.5.0",
    "vue-apollo": "^3.0.0-rc.6",
    "vue-i18n": "^8.14.1",
    "vue-simple-calendar": "^4.2.1",
    "vue2-google-maps": "^0.10.7",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.6.2",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@quasar/app": "^1.1.2",
    "@quasar/cli": "^1.0.0",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "concurrently": "^4.1.2",
    "cross-env": "^6.0.0",
    "cypress": "^3.4.1",
    "cypress-cucumber-preprocessor": "^1.16.0",
    "eslint": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.5",
    "jest-junit": "^8.0.0",
    "jwt-decode": "^2.2.0",
    "lolex": "^4.2.0",
    "majestic": "^1.6.0",
    "moment": "^2.24.0",
    "strip-ansi": "5.2.0",
    "vue-template-compiler": "^2.6.10",
    "wait-on": "^3.3.0",
    "wallaby-vue-compiler": "^1.0.6"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix",
      "pre-push": "yarn lint --fix && yarn test:unit"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
