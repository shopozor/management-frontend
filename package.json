{
  "name": "shopozor-manager",
  "version": "1.0.0",
  "description": "GÃ©rez vos ventes en Budzonnerie",
  "productName": "Gestion de Budzonnerie",
  "cordovaId": "org.cordova.quasar.app",
  "author": "Softozor",
  "private": true,
  "scripts": {
    "build": "quasar build",
    "cypress:ci": "wait-on http://localhost:4000 && cypress run --env configFile=acceptance",
    "cypress:e2e": "wait-on http://localhost:4000 && cypress run --env configFile=e2e",
    "start:ci": "concurrently \"yarn cypress:ci\" --kill-others \"yarn start\" --success first",
    "start:e2e": "concurrently \"yarn cypress:e2e\" --kill-others \"yarn start\" --success first",
    "lint": "eslint --ext .js,.vue src",
    "start": "quasar serve dist/spa -H '0.0.0.0' -p 4000",
    "start:dev": "quasar dev",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit:ci": "jest --updateSnapshot --ci --reporters=jest-junit",
    "test:unit": "jest  --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "jest-junit": {
    "outputDirectory": "./junit-reports",
    "outputName": "junit.xml"
  },
  "dependencies": {
    "@quasar/extras": "^1.1.1",
    "animate.css": "^3.7.0",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-http": "^1.5.14",
    "google-translate-api": "^2.3.0",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "node-fetch": "^2.3.0",
    "quasar": "^1.0.0-beta.17",
    "vue-apollo": "^3.0.0-beta.26",
    "vue-i18n": "^8.10.0",
    "vue-simple-calendar": "^4.2.1",
    "vue2-google-maps": "^0.10.6",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.4.3",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@quasar/app": "^1.0.0-beta.18",
    "@quasar/cli": "^1.0.0-beta.4",
    "@quasar/quasar-app-extension-testing": "^1.0.0-rc.5",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0-beta.22",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "cypress-cucumber-preprocessor": "^1.11.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest-junit": "^6.4.0",
    "jwt-decode": "^2.2.0",
    "majestic": "^1.4.1",
    "moment": "^2.24.0",
    "strip-ansi": "=3.0.1",
    "vue-template-compiler": "^2.6.10",
    "wait-on": "^3.2.0",
    "wallaby-vue-compiler": "^1.0.5"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
